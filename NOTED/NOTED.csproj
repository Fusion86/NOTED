<Project Sdk="Dalamud.NET.Sdk/12.0.2">
	<!-- Assembly Configuration -->
	<PropertyGroup>
		<AssemblyName>NOTED</AssemblyName>
		<Version>1.5.0.0</Version>
	</PropertyGroup>

	<ItemGroup>
		<Compile Remove="Media\**" />
		<None Remove="Media\**" />
